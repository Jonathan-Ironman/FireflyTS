    "@types/node": "^22.15.29",
    "coveralls": "^3.1.1",
    "eslint": "^9.39.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.4.0",
